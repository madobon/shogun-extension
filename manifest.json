{
  "manifest_version": 2,
  "name": "将軍弁当インフォ",
  "version" : "1.0.0",
  "description" : "将軍弁当のChrome拡張です。",
  "permissions": [ "http://bento-shogun.jp/*" ],
  "icons": {
        "16": "icons/shogun.png",
        "48": "icons/shogun.png",
        "128": "icons/shogun.png"
  },
  "browser_action": {
    "default_icon": "icons/shogun.png",
    "default_title": "将軍弁当インフォ",
    "default_popup": "html/popup.html"
  }
}